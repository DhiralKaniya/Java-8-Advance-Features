package com.test.lambda;

public class Lambda {
	
	
	static LambdaInterface li = ()->{
		System.out.println("Test printing");
	};
	
	public static void main(String[] args) {
		li.newPrintMethod();
		Sum addition = (a,b)-> a+b;
		int a = addition.sum(10, 20);
		LambdaInterface l2 = new LambdaInterface() {

			@Override
			public void newPrintMethod() {
				System.out.println("Print l2");
				
			}
			
		};
		l2.newPrintMethod();
	}
}
