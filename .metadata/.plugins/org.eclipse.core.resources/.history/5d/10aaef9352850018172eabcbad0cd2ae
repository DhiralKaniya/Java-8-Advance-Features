package com.session.methodInterface;

import java.util.Arrays;
import java.util.List;
import java.util.function.BiFunction;
import java.util.function.Consumer;

import com.session.utility.Person;

public class TestProgram {
	public static void main(String[] args) {
		String s = new String("Hello world");
		
		List<Person> persons = Arrays.asList(
				new Person(1,"Test1",10),
				new Person(2,"Test2",20),
				new Person(3,"Test3",30)
				);
		
		//reference to a static method 
		persons.forEach(System.out::print);
		
		//Reference to an instance method of particular object
		//persons.forEach(TestProgram::printPerson);
		
		
	}
	
	public static void printPerson(Person p ) {
		System.out.println(p);
	}
}
